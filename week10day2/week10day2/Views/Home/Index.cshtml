@{
    ViewBag.Title = "Home Page";
}





<div ng-app="FactoriesController">
    <div>

    </div>
    Search: <input type="text" ng-model="searchText" />
    {{ searchText}}
    <br />
    <h3>ClassMates:</h3>
    <table>
        <tr ng-repeat="fact in factories | filter: searchText">
            <td>{{fact.name}}</td>
        </tr>
    </table>

</div>
@section scripts{
<script>
    var app = angular.module('factoriesApp', []);
    app.controller('FactoriesController', function ($scope) {
        $scope.factories = [
            { "id": 1, "name": "Josh", },
            { "id": 2, "name": "Bob", },
            { "id": 3, "name": "Cason", },
            { "id": 4, "name": "Baxley", },
            { "id": 5, "name": "Justin", }
        ];
    });
</script>
}














